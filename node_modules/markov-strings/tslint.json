{
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "semicolon": [true, "never"],
      "trailing-comma": [true, "never"],
      "no-console": true,
      "object-literal-sort-keys": false,
      "one-line": false,
      "align": false,
      "typedef": [
          true,
          "call-signature"
      ],
      "no-unused-expression": false,
      "interface-over-type-literal": false,
      "quotemark": [true, "single", "avoid-escape"],
      "no-var-requires": false,
      "arrow-parens": false
  },
  "rulesDirectory": []
}